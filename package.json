{
  "name": "vscode-vega-viewer-yaml",
  "displayName": "Vega Viewer with yaml",
  "description": "VSCode extension for Interactive Preview of Vega & Vega-Lite maps 🗺️ & graphs 📈 ",
  "version": "9.5.1",
  "icon": "images/vega-viewer.png",
  "publisher": "ihgs",
  "author": "isao",
  "contributors": [
    "Taras Novak"
  ],
  "license": "Apache-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/ihgs/vscode-vega-viewer"
  },
  "bugs": "https://github.com/ihgs/vscode-vega-viewer/issues",
  "homepage": "https://github.com/ihgs/vscode-vega-viewer/README.md",
  "keywords": [
    "vega",
    "vega-lite",
    "viewer",
    "preview",
    "charts 📊",
    "maps 🗺️",
    "graphs 📈",
    "dataViz",
    "vegaViz",
    "dev tools 🔬",
    "visual vocabulary 📘",
    "chart examples",
    "interactive preview",
    "svg chart",
    "png chart",
    "gist vega chart"
  ],
  "engines": {
    "vscode": "^1.62.0",
    "node": ">=14.16.0"
  },
  "categories": [
    "Data Science",
    "Formatters",
    "Programming Languages",
    "Other",
    "Snippets",
    "Visualization"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:vega.create_yaml",
    "onCommand:vega.preview_yaml",
    "onCommand:vega.preview_yaml.remote",
    "onCommand:vega.visual.vocabulary",
    "onCommand:vega.examples",
    "onCommand:vega.examples.lite",
    "onWebviewPanel:vega.preview_yaml",
    "onWebviewPanel:vega.visual.vocabulary",
    "onLanguage:json"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "vega.create_yaml",
        "title": "Create Vega Spec",
        "category": "Vega"
      },
      {
        "command": "vega.preview_yaml",
        "title": "Preview Vega Graph",
        "category": "Vega",
        "icon": {
          "light": "./images/vega-viewer.svg",
          "dark": "./images/vega-viewer.svg"
        }
      },
      {
        "command": "vega.preview_yaml.remote",
        "title": "Preview Remote Vega Graph",
        "category": "Vega"
      },
      {
        "command": "vega.visual_yaml.vocabulary",
        "title": "Visual Vocabulary Examples",
        "category": "Vega"
      },
      {
        "command": "vega.examples",
        "title": "Examples",
        "category": "Vega"
      },
      {
        "command": "vega.examples.lite",
        "title": "Lite Examples",
        "category": "Vega"
      }
    ],
    "keybindings": [
      {
        "command": "vega.create_yaml",
        "key": "ctrl+alt+c"
      },
      {
        "command": "vega.preview_yaml",
        "key": "ctrl+alt+v"
      },
      {
        "command": "vega.preview_yaml.remote",
        "key": "ctrl+alt+r"
      },
      {
        "command": "vega.visual_yaml.vocabulary",
        "key": "ctrl+alt+q"
      },
      {
        "command": "vega.examples",
        "key": "ctrl+alt+x"
      },
      {
        "command": "vega.examples.lite",
        "key": "ctrl+alt+l"
      }
    ],
    "languages": [
      {
        "id": "yaml",
        "aliases": [
          "YAML",
          "Vega",
          "vg",
          "vl"
        ],
        "extensions": [
          ".vg.yaml",
          ".vl.yaml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vg.yaml|vl.yaml)/",
          "command": "vega.preview_yaml",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "when": "resourceFilename =~ /.*\\.(vg.yaml|vl.yaml)/",
          "command": "vega.preview_yaml",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceFilename =~ /.*\\.(vg.yaml|vl.yaml)/",
          "command": "vega.preview_yaml",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Vega Viewer for yaml",
      "properties": {
        "vega.viewer_yaml.dataPreviewCommand": {
          "type": "string",
          "enum": [
            "vscode.open",
            "data.preview"
          ],
          "default": "vscode.open",
          "description": "Vega spec CSV & JSON data files preview command"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.2",
    "vscode-test": "^1.6.1"
  },
  "extensionPack": [
    "vsls-contrib.gistfs"
  ],
  "dependencies": {
    "json-stringify-pretty-compact": "^3.0.0",
    "lz-string": "^1.4.4",
    "yaml": "^2.0.1"
  }
}
